<!DOCTYPE html>
<html>
  <head>
    <title>Project3</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="jumbotron">
      <%- if user_signed_in? %>
        <span id="sign"><%= link_to('Logout', destroy_user_session_path, :method => :delete) %> </span>
      <% else %>
        <span id="sign"><%= link_to('Log In', new_user_session_path) %></span>
        <span id="sign"><%= link_to('Sign Up', new_user_registration_path) %></span>
      <% end %>
      <div class="display-3"> Referral </div>
    </div>
    <%= notice %>
    <%= alert %>

    <%= yield %>
  </body>
</html>


