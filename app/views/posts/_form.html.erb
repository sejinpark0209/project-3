<%= form_for @post, :url => { :action => "create"} do |f| %>
  <div class="form-group">
    <%= f.label :contact %><br />
    <%= f.text_field :contact, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :referral_fee %><br />
    <%= f.text_field :p_ref_fee, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :ratio_me %><br />
    <%= f.text_field :ratio_me, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :ratio_you %><br />
    <%= f.text_field :ratio_me, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Expiration_Date %><br />
    <%= f.text_field :exp_date, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.submit "Save", class: "btn btn-block btn-success" %>
  </div>
<% end %>
